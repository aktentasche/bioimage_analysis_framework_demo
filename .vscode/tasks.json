// tasks.json
{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "quasar-dev",
      "type": "shell",
      "command": "${workspaceFolder}/frontend/node_modules/.bin/quasar dev",
      "options": {
        "cwd": "${workspaceFolder}/frontend"
      },
      "presentation": {
        "reveal": "always",
        "panel": "new",
        "focus": true
      }
    },   
  ]
}
